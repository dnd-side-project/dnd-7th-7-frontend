{
  "expo": {
    "name": "dnd-project-running-map",
    "slug": "dnd-project-running-map",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD958IgSFaPAPVN4JNcZ_2HH2DMxGHXJ3M"
      }
    },
    "android": {
      "permissions": ["ACCESS_BACKGROUND_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD958IgSFaPAPVN4JNcZ_2HH2DMxGHXJ3M"
        }
      },
      "package": "com.kmjnnhyk.dndprojectrunningmap"
    },
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/images/loading.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "리뷰 등록을 위해 사진 라이브러리에 접근하도록 허용합니다."
        }
      ]
    ]
  }
}
